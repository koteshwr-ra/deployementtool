<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/size_3"/>
            <solid android:color="@color/clr_F5F5F5"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/size_3"/>
                <gradient android:startColor="@color/clr_0560FD" android:endColor="@color/clr_0560FD" android:angle="0"/>
            </shape>
        </clip>
    </item>
</layer-list>
