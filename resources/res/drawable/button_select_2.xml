<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/size_10"/>
            <stroke android:width="@dimen/size_1" android:color="@color/clr_80196DFD" android:dashGap="@dimen/size_0"/>
            <padding android:left="@dimen/size_2" android:top="@dimen/size_2" android:right="@dimen/size_2" android:bottom="@dimen/size_2"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/size_10"/>
            <stroke android:width="@dimen/size_1" android:color="@color/clr_196DFD" android:dashGap="@dimen/size_0"/>
            <padding android:left="@dimen/size_2" android:top="@dimen/size_2" android:right="@dimen/size_2" android:bottom="@dimen/size_2"/>
        </shape>
    </item>
</selector>
