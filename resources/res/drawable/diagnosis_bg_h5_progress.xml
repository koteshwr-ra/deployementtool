<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#fff5f5f5" android:endColor="#fff5f5f5" android:angle="270" android:centerY="0.75" android:centerColor="#fff5f5f5"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#0bc5b8" android:endColor="#0bc5b8" android:angle="270" android:centerY="0.75" android:centerColor="#0bc5b8"/>
            </shape>
        </clip>
    </item>
</layer-list>
